{
  "name": "react-animated-components",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "description": "React components for animation",
  "keywords": [
    "react",
    "hooks",
    "animation"
  ],
  "repository": "https://github.com/hellskater/react-animated-components.git",
  "author": "K Srinivas Rao (https://twitter.com/Srinu53168)",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings 0",
    "clean": "git clean -xdf dist node_modules",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "framer-motion": "^10.18.0",
    "tailwind-merge": "^2.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "18.16.0",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "eslint": "^8.47.0",
    "eslint-config": "0.0.1",
    "react": "18.2.0",
    "tsconfig": "0.0.1",
    "tsup": "6.7.0",
    "tsup-config": "0.0.1",
    "typescript": "^5.1.6",
    "vitest": "^0.30.1",
    "zod": "^3.21.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "../config/eslint/base",
      "../config/eslint/react"
    ]
  },
  "prettier": {}
}
